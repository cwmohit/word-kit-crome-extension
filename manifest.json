
{
    "manifest_version": 3,
    "name": "word-kit",
    "version": "1.0",
    "description": "Search Wikipedia from the context menu",
    "permissions": ["contextMenus", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
        "128": "128.png"
    },
    "action": {
        "default_popup": "popup.html"
    }
}